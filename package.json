{
		"name": "vas-vendor-v2",
		"version": "1.1.0",
		"description": "Vas vendor APIs",
		"main": "application/index.js",
		"scripts": {
				"start": "npm run build && npm run link && cross-env NODE_ENV=staging node application/server.js",
				"start:staging": "npm run build && npm run link && cross-env NODE_ENV=staging node application/server.js",
				"test": "jest",
				"test:watch": "jest --watch",
				"coverage": "jest --coverage",
				"build": "tsc -p tsconfig.build.json",
				"format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"src/**/*.js\" \"views/**/*.ejs\"",
				"lint": "tslint -p tsconfig.json",
				"lint:fix": "tslint -p tsconfig.json --fix",
				"link": "link-module-alias",
				"nodemon": "nodemon",
				"commit": "git-cz",
				"prepare": "husky install",
				"postbuild": "link-module-alias",
				"preinstall": "command -v link-module-alias && link-module-alias clean || true"
		},
		"_moduleAliases": {
				"@controllers": "./application/controllers",
				"@libs": "./application/libs",
				"@config": "./application/config",
				"@connections": "./application/connections",
				"@routes": "./application/api/routes",
				"@models": "./application/models",
				"@modules": "./application/modules",
				"@api": "./application/api",
				"@components": "./application/components"
		},
		"_moduleAliasIgnoreWarning": true,
		"author": "Peter Iyiola",
		"license": "ISC",
		"devDependencies": {
				"@types/bcrypt": "^3.0.0",
				"@types/cls-hooked": "^4.3.0",
				"@types/cron": "^1.7.2",
				"@types/dotenv": "^8.2.0",
				"@types/ejs": "^3.0.4",
				"@types/express": "^4.17.1",
				"@types/flat": "^5.0.1",
				"@types/helmet": "0.0.47",
				"@types/jest": "^25.2.3",
				"@types/jsonwebtoken": "^8.5.0",
				"@types/lodash": "^4.14.155",
				"@types/lodash.startcase": "^4.4.6",
				"@types/mongoose": "5.10.5",
				"@types/mongoose-paginate-v2": "1.0.4",
				"@types/morgan": "^1.9.0",
				"@types/node": "^14.0.24",
				"@types/nodemailer": "^6.4.0",
				"@types/rimraf": "^3.0.0",
				"@types/seedrandom": "^3.0.2",
				"@types/shortid": "0.0.29",
				"@types/uuid": "^8.0.0",
				"@types/validator": "^13.0.0",
				"@typescript-eslint/eslint-plugin": "^5.59.11",
				"@typescript-eslint/parser": "^5.59.11",
				"cross-env": "^7.0.2",
				"eslint": "^8.42.0",
				"eslint-config-prettier": "^8.8.0",
				"eslint-plugin-prettier": "^4.2.1",
				"husky": "^7.0.2",
				"jest": "^26.0.1",
				"link-module-alias": "^1.2.0",
				"lint-staged": "^11.1.4",
				"nodemon": "^1.19.4",
				"prettier": "^2.0.5",
				"ts-jest": "^26.1.0",
				"tslint": "^6.1.3",
				"tslint-config-prettier": "^1.18.0",
				"typescript": "^4.5.5"
		},
		"dependencies": {
				"@typegoose/typegoose": "^7.2.0",
				"@types/graphql": "^14.5.0",
				"@weaverkit/data": "0.0.11",
				"@weaverkit/errors": "0.0.11",
				"@weaverkit/express": "^0.1.1",
				"@weaverkit/logger": "0.0.11",
				"apollo-server-express": "^2.14.3",
				"axios": "^0.19.2",
				"bcrypt": "^5.1.0",
				"cachegoose": "^8.0.0",
				"cacheman-redis": "^2.1.0",
				"class-validator": "^0.12.2",
				"cls-hooked": "^4.2.2",
				"comment-json": "^4.1.1",
				"cron": "^1.8.2",
				"cryptr": "^6.0.2",
				"dotenv": "^8.2.0",
				"easy-soap-request": "^5.2.0",
				"ejs": "^3.1.3",
				"express": "^4.17.1",
				"express-limit": "^0.1.0",
				"express-validator": "^6.5.0",
				"fh-cls-mongoose": "^2.1.1",
				"flat": "^5.0.0",
				"graphql": "^15.1.0",
				"graphql-mongodb-projection": "^1.1.1",
				"handy-redis": "^1.8.3",
				"helmet": "^3.22.0",
				"json2xls": "^0.1.2",
				"jsonwebtoken": "^8.5.1",
				"lodash": "^4.17.15",
				"lodash.startcase": "^4.4.0",
				"moment": "^2.27.0",
				"moment-timezone": "^0.5.31",
				"mongoose": "~5.10.18",
				"mongoose-lean-virtuals": "0.6.7",
				"mongoose-paginate-v2": "1.3.9",
				"morgan": "^1.10.0",
				"mysql2": "^3.5.1",
				"nanoid": "^3.1.23",
				"openapi3-ts": "^4.1.2",
				"qs": "^6.10.1",
				"reflect-metadata": "^0.1.13",
				"rimraf": "^3.0.2",
				"seedrandom": "^3.0.5",
				"sequelize": "^6.32.1",
				"sequelize-typescript": "^2.1.5",
				"shortid": "^2.2.15",
				"sqlite3": "^5.1.6",
				"string-interpolation": "^1.2.5",
				"string-strip-html": "^6.3.0",
				"strong-soap": "^3.4.1",
				"type-graphql": "^1.0.0-rc.2",
				"uuid": "^8.1.0",
				"validation-chain": "^0.1.5",
				"validator": "^13.0.0"
		},
		"nodemonConfig": {
				"ignore": [
						"**/*.test.ts",
						"**/*.spec.ts",
						".git",
						"node_modules"
				],
				"watch": [
						"src"
				],
				"exec": "npm run start:staging",
				"ext": "ts"
		},
		"lint-staged": {
				"*.{ts,tsx,jsx}": [
						"eslint --fix",
						"prettier --write",
						"git add"
				],
				"!(assets)/**/.{js,jsx}": [
						"eslint --fix",
						"prettier --write",
						"git add"
				]
		},
		"husky": {
				"hooks": {
						"pre-commit": "lint-staged && npm run build",
						"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
				}
		},
		"config": {
				"commitizen": {
						"path": "./node_modules/cz-conventional-changelog"
				}
		}
}
